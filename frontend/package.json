{
  "name": "streamchaser",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "playwright test",
    "htest": "playwright test --headed",
    "lint": "prettier --ignore-path ../.gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path ../.gitignore --write --plugin-search-dir=. .",
    "generate-types": "openapi --input http://api.localhost/openapi.json  --exportCore false --exportServices false --indent 2 --output ./src/lib/generated/",
    "generate-types-go": "openapi --input http://apiv2.localhost/docs/doc.json  --exportCore false --exportServices false --indent 2 --output ./src/lib/generated/go/",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "type": "module",
  "dependencies": {
    "@playwright/experimental-ct-svelte": "^1.29.1",
    "daisyui": "^2.46.0",
    "openapi-typescript-codegen": "^0.23.0",
    "playwright": "^1.29.1",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-media-query": "^1.1.2",
    "svelte-select": "https://github.com/AndreasPB/svelte-select",
    "swiper": "^8.4.5",
    "theme-change": "^2.2.0"
  }
}
