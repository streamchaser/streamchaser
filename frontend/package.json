{
  "name": "streamchaser",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "playwright test",
    "htest": "playwright test --headed",
    "lint": "prettier --ignore-path ../.gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path ../.gitignore --write --plugin-search-dir=. .",
    "generate-types": "openapi --input http://api.localhost/openapi.json  --exportCore false --exportServices false --indent 2 --output ./src/lib/generated/",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.2.4",
    "@sveltejs/adapter-vercel": "^3.0.0",
    "@sveltejs/kit": "^1.18.0",
    "@tsconfig/svelte": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.7"
  },
  "type": "module",
  "dependencies": {
    "@playwright/test": "^1.33.0",
    "@splidejs/svelte-splide": "^0.2.9",
    "daisyui": "^3.1.7",
    "openapi-typescript-codegen": "^0.24.0",
    "playwright": "^1.33.0",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-media-query": "^1.1.1",
    "svelte-select": "https://github.com/AndreasPB/svelte-select",
    "theme-change": "^2.0.2"
  }
}
