{
  "name": "streamchaser",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "playwright test",
    "htest": "playwright test --headed",
    "lint": "prettier --ignore-path ../.gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path ../.gitignore --write --plugin-search-dir=. .",
    "generate-types": "openapi --input http://api.localhost/openapi.json  --exportCore false --exportServices false --indent 2 --output ./src/lib/generated/",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/adapter-vercel": "^2.4.1",
    "@sveltejs/kit": "^1.14.0",
    "@tsconfig/svelte": "^4.0.1",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "svelte": "^3.57.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.4.4",
    "vite": "^4.2.1"
  },
  "type": "module",
  "dependencies": {
    "@playwright/experimental-ct-svelte": "^1.28.1",
    "daisyui": "^2.38.1",
    "openapi-typescript-codegen": "^0.23.0",
    "playwright": "^1.28.1",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-media-query": "^1.1.1",
    "svelte-select": "https://github.com/AndreasPB/svelte-select",
    "swiper": "^8.0.7",
    "theme-change": "^2.0.2"
  }
}
